GET sec_logs/_eql/search
{
  "event_type_field": "event.category",
  "timestamp_field": "@timestamp",
  "filter": {
    "range" : {
      "file.size" : {
        "gte" : 1,
        "lte" : 1000000
      }
    }
  },
  "query": """
    file where (file.type == "file" and file.name == "cmd.exe")
  """
}
